{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "Ecoji/Ecoji.csproj"
          ],
          "exclude": []
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "globalNamespaceId": "Ecoji .NET",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appFaviconPath": "images/icon@2x.png",
      "_appTitle": "Ecoji .NET",
      "_appFooter": "Copyright 2020 Aaron Bockover"
    }
  }
}